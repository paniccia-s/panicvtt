{% extends "base" %}

{% block content %}
<form action="disconnect/" method="post">
    <button type="submit">Disconnect</button>
</form>

<form action="/vtt" method="post">
    <input type="text" name="command" id="command" placeholder="Command...">
    <button type="submit">Submit</button>
</form>

<ul>
    {% for i in items %}
        <li>{{i}}</li>
    {% endfor %}
</ul>
{% endblock content %}
